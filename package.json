{
  "name": "WellbeingTracker",
  "version": "1.0.0",
  "description": "it's a real sweet app",
  "main": "index.js",
  "scripts": {
    "build": "browserify . -o bundle.js -g uglifyify",
    "start": "budo . -d --serve bundle.js --live",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": []
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/melissa-c/WellbeingTracker"
  },
  "author": "melissa-c",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/melissa-c/WellbeingTracker/issues"
  },
  "homepage": "https://github.com/melissa-c/WellbeingTracker",
  "dependencies": {
    "express": "^4.13.4"
  },
  "devDependencies": {
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
